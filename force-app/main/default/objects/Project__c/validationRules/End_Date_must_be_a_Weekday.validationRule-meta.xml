<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>End_Date_must_be_a_Weekday</fullName>
    <active>true</active>
    <description>End_Date_must_be_a_Weekday</description>
    <errorConditionFormula>(WEEKDAY( End_Date__c ) == 1) || (WEEKDAY( End_Date__c ) == 7)</errorConditionFormula>
    <errorMessage>End_Date_must_be_a_Weekday</errorMessage>
</ValidationRule>
